    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="android:background" >@color/dark_green</item>
        <item name="background">@color/dark_green</item>
        <item name="android:titleTextStyle">@style/actionbar_title</item>
        <item name="titleTextStyle">@style/actionbar_title</item>
        <item name="android:elevation">@dimen/elevation</item>
        <item name="elevation">@dimen/elevation</item>
    </style>
    <style name="QText" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:textColor">@color/glossy_black</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="actionbar_title" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/pure_white</item>
    </style>
