    <style name="outer_layout">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin</item>
    </style>
    <style name="normal_button">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:textColor">@drawable/abc_button_text</item>
        <item name="android:layout_width">wrap_content</item><item name="android:layout_height">wrap_content</item><item name="android:background">@drawable/abc_button_background</item>
         <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/normal_button_padding_top</item>
        <item name="android:paddingBottom">@dimen/normal_button_padding_bottom</item>
        <item name="android:paddingLeft">@dimen/normal_button_padding_left</item>
        <item name="android:paddingRight">@dimen/normal_button_padding_right</item>
        <item name="android:textSize">@dimen/text_size</item>
    </style>
    <style name="full_screen_button" parent="normal_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="flat_button">
        <item name="android:layout_weight">26</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/abc_flat_button_background</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:textColor">@drawable/abc_button_text</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_size</item>
    </style>
    <style name="edit_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:paddingTop">@dimen/normal_button_padding_top</item>
        <item name="android:paddingBottom">@dimen/normal_button_padding_bottom</item>
        <item name="android:paddingLeft">@dimen/normal_button_padding_left</item>
        <item name="android:paddingRight">@dimen/normal_button_padding_right</item>
        <item name="android:background">@drawable/abc_edit_text_background</item>
    </style>
    <style name="edit_text.phone"><item name="android:inputType">phone</item></style>
    <style name="edit_text.name"><item name="android:inputType">textPersonName</item></style>
    <style name="edit_text.address"><item name="android:inputType">textPostalAddress</item></style>
    <style name="edit_text.email"><item name="android:inputType">textEmailAddress</item></style>
    <style name="edittext_fragment">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/abc_edit_text_background</item>
    </style>

    <style name="highlighted_textview" parent="vertical_LL.sm.1.center">
        <item name="android:background">@drawable/abc_textview_background_border</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
    </style>
    <style name="highlighted_picker" parent="horizontal_LL.sm.4">
        <item name="android:background">@drawable/abc_edit_text_background</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
